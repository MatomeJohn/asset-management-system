{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {},
  "git": {
    "deploymentEnabled": true
  },
  "routes": [
    {
      "src": "^/assets/.*\\..*$",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*)\\..*$",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
